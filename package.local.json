{
  "name": "joke-discord",
  "version": "1.0.0",
  "description": "generate joke on discord server using jokeapi",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "serve": "node build/main.js",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/P-Bunnet/joke-discord.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/P-Bunnet/joke-discord/issues"
  },
  "homepage": "https://github.com/P-Bunnet/joke-discord#readme",
  "dependencies": {
    "@discordx/importer": "^1.1.9",
    "@discordx/pagination": "^2.1.2",
    "axios": "^0.27.2",
    "discord-api-types": "^0.30.0",
    "discord.js": "^13.7.0",
    "discordx": "^9.6.0",
    "dotenv": "^16.0.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/node": "^17.0.33",
    "nodemon": "^2.0.18",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
